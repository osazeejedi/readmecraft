{
  "name": "readmecraft",
  "version": "1.0.0",
  "description": "A powerful CLI and web tool for generating README sections, badges, and starter templates",
  "main": "dist/cli.js",
  "bin": {
    "readmecraft": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/cli.js",
    "typecheck": "tsc --noEmit"
