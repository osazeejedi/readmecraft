    "documentation-tool",
    "shields-io",
    "automation",
    "typescript"
  ],
  "author": "ReadmeCraft Contributors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/readmecraft.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/readmecraft/issues"
  },
  "homepage": "https://github.com/yourusername/readmecraft#readme",
  "dependencies": {
    "chalk": "^4.1.2",
    "commander": "^11.1.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=14.0.0"
